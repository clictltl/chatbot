{
  "name": "vite-vue-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:github": "vue-tsc -b && vite build --mode github",
    "build:wordpress": "vue-tsc -b && vite build --mode production",
    "build": "npm run build:wordpress",
    "preview": "vite preview",
    "deploy": "npm run build:github && touch dist/.nojekyll"
  },
  "dependencies": {
    "leader-line-new": "^1.1.9",
    "vue": "^3.4.38"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^5.1.3",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "vue-tsc": "^2.1.4"
  }
}
